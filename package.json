{
  "name": "tstailor",
  "version": "0.0.0",
  "description": "CLI tool + API to generate TypeScript interfaces from raw JSON objects",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "rimraf dist && tsup --config tsup.config.ts",
    "release": "pnpm build && changeset publish",
    "lint:fix": "biome check --apply ./src",
    "lint:fix-unsafe": "biome check --apply-unsafe ./src",
    "lint": "biome lint ./src && tsc",
    "test": "vitest",
    "prepare": "husky"
  },
  "keywords": [
    "code generation",
    "interfaces",
    "strict",
    "type",
    "type system",
    "generate",
    "typescript"
  ],
  "author": "ljs360d",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@changesets/cli": "^2.27.1",
    "@types/chai": "^4.3.11",
    "@types/node": "^20.11.0",
    "chai": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "copyfiles": "^2.4.1",
    "husky": "^9.0.11",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "changeset": {
    "defaultBranch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "tstailor": "./dist/cli/cli.mjs",
    "tstl": "./dist/cli/cli.mjs",
    "tsify": "./dist/cli/cli.mjs"
  }
}
